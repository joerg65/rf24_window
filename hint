avrdude:

//write first byte in eeprom to ff
avrdude -pt84 -cavrispv2 -P/dev/ttyACM0 -U eeprom:w:0xff:m

//write flash
avrdude -pt84 -cavrispv2 -P/dev/ttyACM0 -U flash:w:rf24_window.hex

avrdude -cavrisp2 -P/dev/ttyACM0 -pt84 -Uflash:w:rf24_window.hex:a -Ulfuse:w:0xe2:m -Uhfuse:w:0xdf:m -Uefuse:w:0xfe:m
			